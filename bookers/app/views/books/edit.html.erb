<h1>Editing Book</h1>
<!-- form_with部分 -->
<%= form_with model:@book, url:"/books/#{@list.id}", local:true do |f| %>
 <h4>Title</h4>
 <%= f.text_field :title %>
 <h4>Body</h4>
 <%= f.text area :body %>
 <%= f.submit 'Update Book' %>
 <%= link_to "Show", show_book_path(@book.id) %>
 <%= link_to "Back", index_book_path(@book.id) %>
<% end %>